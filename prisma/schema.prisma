generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("connectionString")
}

model users {
  id       Int     @id @default(autoincrement())
  username String? @db.VarChar(255)
  password String?
  habit    habit[]
}

model habit {
  id     Int    @id @default(autoincrement())
  name   String @db.VarChar(255)
  icon   Int?
  user   users  @relation(fields: [userId], references: [id])
  userId Int
}

model session {
  id        String   @id
  sid       String   @unique
  data      String
  expiresAt DateTime
}
