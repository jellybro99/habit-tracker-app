generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("connectionString")
}

model users {
  id       Int     @id @default(autoincrement())
  username String? @db.VarChar(255)
  password String? @db.VarChar(255)
}

model session {
  id        String  @id
  sid       String  @unique
  data      String
  expiresAt DateTime
}